<ads:BannerTargetingCanvas xmlns:ads="http://adserversoft.com"
                           xmlns:mx="http://www.adobe.com/2006/mxml"
                           xmlns:component="com.adserversoft.flexfuse.client.view.component.*"
                           xmlns:canvas="com.adserversoft.flexfuse.client.view.canvas.*"
                           width="100%"
                           height="100%"
                           creationPolicy="all">
    <mx:VBox paddingRight="4" paddingBottom="3" paddingTop="0" paddingLeft="4" width="100%" height="100%">
        <mx:TabNavigator id="tabNavigator" width="100%" height="100%">
            <canvas:BannerTargetingTimeCanvasUI id="bannerTargetingTimeCanvas" mode="{mode}" label="Days of Week"/>
            <canvas:BannerTargetingTimeHourCanvasUI id="bannerTargetingTimeHourCanvas" mode="{mode}" label="Time of Day"/>
            <canvas:BannerTargetingCappingCanvasUI id="bannerTargetingCappingCanvas" mode="{mode}" label="Capping"/>
            <canvas:BannerTargetingGeoCanvasUI id="bannerTargetingGeoCanvas" mode="{mode}" label="Geo"/>
        </mx:TabNavigator>
    </mx:VBox>

</ads:BannerTargetingCanvas>